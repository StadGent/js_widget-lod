{
  "name": "@district09/lod-widgets",
  "version": "1.3.3",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/lod-widgets/lod-widgets.esm.js",
  "exports": {
    ".": {
      "import": "./dist/lod-widgets/lod-widgets.esm.js",
      "require": "./dist/lod-widgets/lod-widgets.cjs.js"
    },
    "./lod-cards": {
      "import": "./dist/components/lod-cards.js",
      "types": "./dist/components/lod-cards.ts"
    },
    "./lod-opening-hours": {
      "import": "./dist/components/lod-opening-hours.js",
      "types": "./dist/components/lod-opening-hours.ts"
    },
    "./lod-decision-card": {
      "import": "./dist/components/lod-decision-card.js",
      "types": "./dist/components/lod-decision-card.ts"
    },
    "./lod-decisions-list": {
      "import": "./dist/components/lod-decisions-list.js",
      "types": "./dist/components/lod-decisions-list.ts"
    },
    "./lod-regulations-list": {
      "import": "./dist/components/lod-regulations-list.js",
      "types": "./dist/components/lod-regulations-list.ts"
    },
    "./lod-table": {
      "import": "./dist/components/lod-table.js",
      "types": "./dist/components/lod-table.d.ts"
    },
    "./lod-card": {
      "import": "./dist/components/lod-card.js",
      "types": "./dist/components/lod-card.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/StadGent/js_widget-lod.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prettier-fix": "prettier . --write"
  },
  "devDependencies": {
    "@stencil/core": "^4.29.3",
    "@stencil/sass": "^3.2.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "prettier": "^3.5.3",
    "puppeteer": "^24.6.1",
    "@digipolis-gent/opening-hours-widget": "^0.2.0"
  },
  "license": "MIT"
}
